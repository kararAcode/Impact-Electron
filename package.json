{
  "name": "impact-electron",
  "version": "1.1.0",
  "description": "",
  "main": "./src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build-win": "electron-builder --win",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/kararAcode/Impact-Electron",
  "publish": {
    "provider": "github",
    "releaseType": "release",
    "publish": "always"

  },
  "author": {
    "name": "kararAcode",
    "email": "alkarar975@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "electron-log": "^5.1.4",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.2.1",
    "install": "^0.13.0",
    "jquery": "^3.7.1",
    "npm": "^10.8.1",
    "standard-version": "^9.5.0"
  },
  "devDependencies": {
    "electron": "^30.1.1",
    "electron-builder": "^24.13.3",
    "electron-builder-squirrel-windows": "^24.13.3"
  },
  "build": {
    "appId": "com.example.yourapp",
    "files": [
      "src/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        "nsis",
        "squirrel"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    },
    "squirrelWindows": {}
  }
}
